{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": [2, "global"],
    "quotes": [2, "single"],
    "no-trailing-spaces": 1,
    "no-undef": "error",
    "no-use-before-define": [2, "nofunc"],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used" }]
  },
  "global": {
  },
  "ecmaFeatures": {
    "spread": true,
    "restParams": true
  }
}
